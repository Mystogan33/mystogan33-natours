{
  "name": "natours",
  "version": "1.0.0",
  "description": "Landing page for Natours",
  "main": "index.js",
  "scripts": {
    "sass:watch": "node-sass scss/main.scss css/style.css -w",
    "devserver": "live-server",
    "start:dev": "npm-run-all --parallel devserver sass:watch",
    "sass:compile": "node-sass scss/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss css/style.concat.css --use autoprefixer -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "sass:build": "npm-run-all sass:compile concat:css prefix:css compress:css"
  },
  "author": "Mystogan33 (Valentin GAY)",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "concat": "^1.0.3",
    "live-server": "^1.2.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.1",
    "postcss-cli": "^8.0.0"
  }
}
